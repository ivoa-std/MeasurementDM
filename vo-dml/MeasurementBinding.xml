<m:mappedModels xmlns:m="http://www.ivoa.net/xml/vodml-binding/v0.9.1">
    <!-- ========================================
    This is a minimal sample file for mapping VO-DML models to XSD or Java using the gradle tooling
     -->

    <model>
        <name>meas</name>
        <file>Meas-v1.0.vo-dml.xml</file>
        <java-package>org.ivoa.dm.measurement</java-package>
        <xml-targetnamespace prefix="meas" schemaFilename="Measurement.xsd">http://ivoa.net/dm/models/vo-dml/xsd/meas</xml-targetnamespace>
    </model>
</m:mappedModels>